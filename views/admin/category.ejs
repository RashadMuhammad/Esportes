      <div class="main-panel">
        <div class="content-wrapper">
          <div class="page-header">
            <h3 class="page-title"> Category Management </h3>
            <% if (message.length> 0) { %>
              <div class="alert alert-danger" role="alert" id="alertMessage">
                <%= message %>
              </div>
              <script>
                // Hide the alert after 5 seconds (5000 milliseconds)
                setTimeout(function () {
                  var alertElement = document.getElementById('alertMessage');
                  if (alertElement) {
                    alertElement.style.display = 'none'; // Hide the alert
                  }
                }, 5000);
              </script>
              <% } %>


          </div>
          <div class="row">
            <div class="col-lg-12 grid-margin stretch-card">
              <div class="card">
                <div class="card-body">
                  <h4 class="card-title">Categories</h4>
                  <!-- <p class="card-description"> Add class <code>.table</code> -->
                  </p>
                  <div class="table-responsive">
                    <table class="table">
                      <thead>
                        <tr>
                          <th>Sl.No</th>
                          <th>Name</th>
                          <th>Description</th>
                          <th>Image</th>
                          <th>Action</th>
                        </tr>
                      </thead>
                      <tbody>
                        <% categories.forEach((category, index)=> { %>
                          <tr>
                            <td>
                              <%= index + 1 + ((currentPage - 1) * 10) %> <!-- Adjust index for pagination -->
                            </td>
                            <td>
                              <%= category.name %>
                            </td>
                            <td>
                              <%= category.description %>
                            </td>
                            <td>
                              <% if (category.image) { %>
                                <img src="/uploads/<%= category.image %>" alt="Category Image" width="100" />
                                <% } else { %>
                                  <span>No image</span>
                                  <% } %>
                            </td>
                            <td>
                              <button type="button" class="badge badge-warning" data-toggle="modal"
                                data-target="#editModal<%= category._id %>">
                                Edit
                              </button>


                              <button type="button"
                                class="badge <%= category.isListed ? 'badge-primary' : 'badge-success' %>"
                                data-toggle="modal" data-target="#toggleListingModal-<%= category._id %>">
                                <%= category.isListed ? 'Unlist' : 'List' %>
                              </button>


                              <% categories.forEach((category)=> { %>
                                <div class="modal fade" id="toggleListingModal-<%= category._id %>" tabindex="-1"
                                  role="dialog" aria-labelledby="toggleListingModalLabel" aria-hidden="true">
                                  <div class="modal-dialog" role="document">
                                    <div class="modal-content">
                                      <div class="modal-header">
                                        <h5 class="modal-title" id="toggleListingModalLabel">
                                          <%= category.isListed ? 'Unlist Category' : 'List Category' %>
                                        </h5>
                                        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                                          <span aria-hidden="true">&times;</span>
                                        </button>
                                      </div>
                                      <form action="/admin/Categories/toggle-listing/<%= category._id %>" method="POST">
                                        <div class="modal-body">
                                          Are you sure you want to <%= category.isListed ? 'unlist' : 'list' %> the
                                            category "<%= category.name %>"?
                                        </div>
                                        <div class="modal-footer">
                                          <button type="button" class="btn btn-secondary"
                                            data-dismiss="modal">Cancel</button>
                                          <button type="submit" class="btn btn-primary">
                                            Yes, <%= category.isListed ? 'Unlist' : 'List' %>
                                          </button>
                                        </div>
                                      </form>
                                    </div>
                                  </div>
                                </div>
                                <% }) %>


                                  <!-- =============================== -->

                                  <!-- Button to trigger the delete modal -->
                                  <button type="button" class="badge badge-danger" data-toggle="modal"
                                    data-target="#deleteModal-<%= category._id %>">Delete</button>



                                  <!-- Delete Confirmation Modal for Categories -->
                                  <% categories.forEach((category)=> { %>
                                    <div class="modal fade" id="deleteModal-<%= category._id %>" tabindex="-1"
                                      role="dialog" aria-labelledby="deleteModalLabel" aria-hidden="true">
                                      <div class="modal-dialog" role="document">
                                        <div class="modal-content">
                                          <div class="modal-header">
                                            <h5 class="modal-title" id="deleteModalLabel">Confirm Deletion</h5>
                                            <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                                              <span aria-hidden="true">&times;</span>
                                            </button>
                                          </div>
                                          <form action="/admin/Categories/delete/<%= category._id %>" method="POST">
                                            <div class="modal-body">
                                              Are you sure you want to delete the category "<%= category.name %>"?
                                            </div>
                                            <div class="modal-footer">
                                              <button type="button" class="btn btn-secondary"
                                                data-dismiss="modal">Cancel</button>
                                              <button type="submit" class="btn btn-danger">Yes, Delete</button>
                                            </div>
                                          </form>
                                        </div>
                                      </div>
                                    </div>
                                    <% }) %>

                            </td>
                          </tr>
                          <!-- Unlist Confirmation Modal -->
                          <div class="modal fade" id="unlistModal<%= category._id %>" tabindex="-1" role="dialog"
                            aria-labelledby="exampleModalLabel" aria-hidden="true">
                            <div class="modal-dialog" role="document">
                              <div class="modal-content">
                                <div class="modal-header">
                                  <h5 class="modal-title" id="exampleModalLabel">Change Category Status</h5>
                                  <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                                    <span aria-hidden="true">&times;</span>
                                  </button>
                                </div>
                                <div class="modal-body">
                                  Are you sure you want to <%= category.isListed ? 'unlist' : 'list' %> this
                                    category?
                                </div>
                                <div class="modal-footer">
                                  <button type="button" class="btn btn-secondary" data-dismiss="modal">Cancel</button>
                                  <button type="button" class="btn btn-primary"
                                    onclick="unlistCategory('<%= category._id %>')">
                                    Confirm
                                  </button>
                                </div>
                              </div>
                            </div>
                          </div>

                          <!-- ================================================================================================================== -->

                          <div class="modal fade" id="editModal<%= category._id %>" tabindex="-1" role="dialog"
                            aria-labelledby="editModalLabel<%= category._id %>" aria-hidden="true">
                            <div class="modal-dialog" role="document">
                              <div class="modal-content">
                                <div class="modal-header">
                                  <h5 class="modal-title" id="editModalLabel<%= category._id %>">Edit Category</h5>
                                  <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                                    <span aria-hidden="true">&times;</span>
                                  </button>
                                </div>
                                <div class="modal-body">


                                  <!-- Edit form within modal -->
                                  <form id="editCategoryForm_<%= category._id %>" action="/admin/Categories/edit/<%= category._id %>"
                                    method="POST" enctype="multipart/form-data">
                                  <input type="hidden" name="categoryId" value="<%= category._id %>" id="editCategoryId_<%= category._id %>">
                              
                                  <div class="form-group">
                                    <label for="categoryName_<%= category._id %>">Category Name</label>
                                    <input type="text" class="form-control text text-white" id="categoryName_<%= category._id %>" name="name"
                                           value="<%= category.name %>">
                                    <small id="categoryNameError_<%= category._id %>" class="form-text text-danger"></small>
                                    <!-- Error message for name -->
                                  </div>
                              
                                  <div class="form-group">
                                    <label for="categoryDescription_<%= category._id %>">Category Description</label>
                                    <input type="text" class="form-control" id="categoryDescription_<%= category._id %>" name="description"
                                           value="<%= category.description %>">
                                    <small id="categoryDescriptionError_<%= category._id %>" class="form-text text-danger"></small>
                                    <!-- Error message for description -->
                                  </div>
                              
                                  <div class="form-group">
                                    <label for="categoryImage_<%= category._id %>">Category Image</label>
                                    <input type="file" class="form-control" id="categoryImage_<%= category._id %>" name="image">
                                    <small id="categoryImageError_<%= category._id %>" class="form-text text-danger"></small>
                                    <!-- Error message for image -->
                                    <% if (category.image) { %>
                                      <img src="/uploads/<%= category.image %>" alt="Current Image" width="100" class="mt-2" />
                                    <% } %>
                                  </div>
                              
                                  <div class="modal-footer">
                                    <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
                                    <button type="submit" class="btn btn-primary" id="saveChangesButton_<%= category._id %>">Save changes</button>
                                  </div>
                              </form>
                              

                                </div>
                              </div>
                            </div>
                          </div>
                          <% }) %>
                      </tbody>
                    </table>
